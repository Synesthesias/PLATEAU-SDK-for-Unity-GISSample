                <ui:RadioButton name="Speedx3" text="x3" value="false" class="speed-button" style="margin-top: 0; margin-right: 0; margin-bottom: 0; margin-left: 0; padding-top: 0; padding-right: 0; padding-bottom: 0; padding-left: 0; border-top-right-radius: 20px; border-bottom-right-radius: 20px;" />
